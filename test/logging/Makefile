COMPONENT=LoggingTestC

CFLAGS += -I$(PWD)

PLATFORM_BAUDRATE ?= 500000UL
ifneq ($(PLATFORM_BAUDRATE),0)
	CFLAGS += -DPLATFORM_BAUDRATE=$(PLATFORM_BAUDRATE)
endif

